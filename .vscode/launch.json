    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Mode",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/queue_project",
            "args": [
                "125",
                "457",
                "1.2",
                "1.54",
                "fdkpsod dsd",
                "jodfs",
                "456",
                "-a",
                "--afdsfsa",
                "4d:5s",
                "2s:182M"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build debug"
        }
    ]
}